{
    "version": 2,
    "name": "tg-hketa",
    "env": {
        "TG_KEY": "@tg_key",
        "SENTRY_KEY": "@sentry_key",
        "GA_TID": "@ga_tid",
        "IPG_KEY": "@ipg_key"
    },
    "builds": [
        { "src": "*.js", "use": "@now/node" }
    ],
    "routes": [
      { "src": "/message", "dest": "/index.js" }
    ],
    "regions": ["sin1", "hnd1"]
}  