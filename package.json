{
  "name": "tg-hketa",
  "version": "1.0.1",
  "engines": {
    "node": "12.x"
  },
  "description": "A telegram bot to get the ETA of HK public transport.",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "prod": "now --prod --token=$NOW_TOKEN",
    "local": "now dev",
    "updateRoutes": "node utils/get-routes-NLB.js && node utils/get-routes.js",
    "setWebhook": "curl -F \"url=https://$URL/message\" https://api.telegram.org/bot$TOKEN/setWebhook"
  },
  "keywords": [
    "nodejs",
    "telegram",
    "bot"
  ],
  "author": "Kiros Choi",
  "license": "GPL-3.0-or-later",
  "dependencies": {
    "@sentry/node": "^5.18.1",
    "@types/axios": "^0.14.0",
    "@types/body-parser": "^1.19.0",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.6",
    "apollo-boost": "^0.4.9",
    "axios": "^0.19.2",
    "body-parser": "^1.19.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "graphql": "^15.2.0",
    "graphql-tag": "^2.10.3",
    "moment": "^2.27.0",
    "node-fetch": "^2.6.0",
    "telegraf": "^3.38.0",
    "universal-analytics": "^0.4.20",
    "xml-js": "^1.6.11"
  },
  "devDependencies": {}
}
